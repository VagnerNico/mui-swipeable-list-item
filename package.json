{
  "name": "mui-swipeable-list-item",
  "version": "0.2.2",
  "description": "This is SwipeableListItem based on Material-UI (https://github.com/mui-org/material-ui), this package adds the functionality of swipe left and right for the Material-UI ListItem Component.",
  "scripts": {
    "ts-build": "rm -rf ./dist/** && tsc",
    "babel-build": "rm -rf ./dist/** && babel ./src --out-dir dist --extensions '.ts,.tsx'",
    "semantic-release": "semantic-release",
    "watch": "tsc -- --watch",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o docs",
    "deploy-storybook": "npm run build-storybook && storybook-to-ghpages -- --existing-output-dir=docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VagnerNico/mui-swipeable-list-item.git"
  },
  "bugs": {
    "url": "https://github.com/VagnerNico/mui-swipeable-list-item/issues"
  },
  "homepage": "https://github.com/VagnerNico/mui-swipeable-list-item#readme",
  "files": [
    "dist",
    "README.md"
  ],
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "keywords": [
    "material",
    "design",
    "swipeable",
    "list",
    "item",
    "mui",
    "swipe"
  ],
  "author": "Vagner Jos√© Nicolodi",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.1",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.0",
    "@babel/plugin-transform-typescript": "^7.13.0",
    "@babel/preset-env": "^7.13.5",
    "@babel/preset-typescript": "^7.13.0",
    "@storybook/addon-actions": "^6.1.19",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.1.19",
    "@storybook/addon-viewport": "^6.1.19",
    "@storybook/addons": "^6.1.19",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.1.19",
    "@storybook/storybook-deployer": "^2.8.7",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "babel-loader": "^8.2.2",
    "compression-webpack-plugin": "^7.1.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "jest": "^26.6.3",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-test-renderer": "^17.0.1",
    "semantic-release": "^17.3.9",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.17",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0",
    "typescript": "^4.2.2"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.11.3",
