    "semantic-release": "^17.0.4",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.8.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "@material-ui/core": "*",
